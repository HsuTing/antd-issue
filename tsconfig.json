{
  "compilerOptions": {
    "lib": ["es6", "dom"],
    "noEmit": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "jsx": "react"
  },
  "include": [
    "index.tsx"
  ]
}
